        "/api/admin/users": {
            "get": {
                "tags": [
                    "Users"
                ],
                "summary": "Danh sách người dùng (có phân trang, role, permissions, profile)",
                "description": "Trả về danh sách người dùng dạng phân trang cùng với thông tin chi tiết (profile, roles, permissions).",
                "operationId": "7682ed8f32d8216884bcdfd052b767cd",
                "parameters": [
                    {
                        "name": "page",
                        "in": "query",
                        "description": "Số trang hiện tại",
                        "required": false,
                        "schema": {
                            "type": "integer",
                            "example": 1
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Lấy danh sách người dùng thành công",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "data": {
                                            "type": "array",
                                            "items": {
                                                "properties": {
                                                    "id": {
                                                        "type": "integer",
                                                        "example": 1
                                                    },
                                                    "email": {
                                                        "type": "string",
                                                        "example": "admin@educonnect.com"
                                                    },
                                                    "profile": {
                                                        "properties": {
                                                            "full_name": {
                                                                "type": "string",
                                                                "example": "Admin User"
                                                            },
                                                            "phone_number": {
                                                                "type": "string",
                                                                "example": "385-678-0558"
                                                            },
                                                            "birthday": {
                                                                "type": "string",
                                                                "example": "2001-03-26"
                                                            },
                                                            "gender": {
                                                                "type": "integer",
                                                                "example": 1
                                                            },
                                                            "address": {
                                                                "type": "string",
                                                                "example": "42827 Nannie Loaf, FL 79652-7986"
                                                            },
                                                            "avatar": {
                                                                "type": "string",
                                                                "example": "avatars/default.png"
                                                            }
                                                        },
                                                        "type": "object"
                                                    },
                                                    "roles": {
                                                        "type": "array",
                                                        "items": {
                                                            "type": "string",
                                                            "example": "admin"
                                                        }
                                                    },
                                                    "permissions": {
                                                        "type": "array",
                                                        "items": {
                                                            "type": "string",
                                                            "example": "manage_users"
                                                        }
                                                    },
                                                    "created_at": {
                                                        "type": "string",
                                                        "example": "2025-10-02T10:27:06.000000Z"
                                                    },
                                                    "updated_at": {
                                                        "type": "string",
                                                        "example": "2025-10-02T10:27:06.000000Z"
                                                    }
                                                },
                                                "type": "object"
                                            }
                                        },
                                        "links": {
                                            "properties": {
                                                "first": {
                                                    "type": "string",
                                                    "example": "http://127.0.0.1:8000/api/admin/users?page=1"
                                                },
                                                "last": {
                                                    "type": "string",
                                                    "example": "http://127.0.0.1:8000/api/admin/users?page=44"
                                                },
                                                "prev": {
                                                    "type": "string",
                                                    "example": null,
                                                    "nullable": true
                                                },
                                                "next": {
                                                    "type": "string",
                                                    "example": "http://127.0.0.1:8000/api/admin/users?page=2"
                                                }
                                            },
                                            "type": "object"
                                        },
                                        "meta": {
                                            "properties": {
                                                "current_page": {
                                                    "type": "integer",
                                                    "example": 1
                                                },
                                                "from": {
                                                    "type": "integer",
                                                    "example": 1
                                                },
                                                "last_page": {
                                                    "type": "integer",
                                                    "example": 44
                                                },
                                                "path": {
                                                    "type": "string",
                                                    "example": "http://127.0.0.1:8000/api/admin/users"
                                                },
                                                "per_page": {
                                                    "type": "integer",
                                                    "example": 15
                                                },
                                                "to": {
                                                    "type": "integer",
                                                    "example": 15
                                                },
                                                "total": {
                                                    "type": "integer",
                                                    "example": 646
                                                }
                                            },
                                            "type": "object"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Không có quyền truy cập hoặc chưa đăng nhập"
                    }
                }
            }
        }
    },
    "tags": [
        {
            "name": "Test",
            "description": "Test"
        },
        {
            "name": "Users",
            "description": "Users"
        }
    ]
}